# Class-SNA v2.0 Environment Variables
# Copy this file to .env and fill in your values

# ============================================
# Flask Configuration
# ============================================
FLASK_ENV=production
SECRET_KEY=your-super-secret-key-change-this-in-production

# ============================================
# Google API Keys (comma-separated for rotation)
# ============================================
GOOGLE_API_KEYS=your-api-key-1,your-api-key-2,your-api-key-3

# Gemini Model (default: gemini-2.0-flash)
GEMINI_MODEL=gemini-2.0-flash

# ============================================
# Redis Configuration
# ============================================
REDIS_URL=redis://redis:6379/0
SESSION_TYPE=redis

# ============================================
# Domain Configuration (for SSL/Nginx)
# ============================================
DOMAIN=your-domain.com

# ============================================
# AWS EC2 Specific (Optional)
# ============================================
# AWS_REGION=ap-northeast-2
# ELASTIC_IP=your-elastic-ip

# ============================================
# Logging Level
# ============================================
LOG_LEVEL=INFO
